<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.childsafetyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChildSafetyApp">
        <activity
            android:name=".AddHelpLine">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fetchdata" />
        <activity android:name=".Admin" />
        <activity android:name=".Home" />
        <activity android:name=".Register" />
        <activity android:name=".LogIn" />
        <activity
            android:name=".ListOrganisationActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity
            android:name=".AddOrganisationActivity"
            android:label="@string/add_new_organisation"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Light.NoTitleBar" />
        <activity
            android:name=".AddChildActivity"
            android:label="@string/add_new_employee"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Light.NoTitleBar" />
        <activity
            android:name=".ListChildActivity"
            android:label="@string/all_children"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Light.NoTitleBar" />
    </application>

</manifest>